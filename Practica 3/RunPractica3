package Practica3;

public class RunPractica3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        Calculadora casio = new Calculadora();
        Television sony = new Television();
        Radio rd = new Radio();
        
        System.out.println(casio.encender());
        System.out.println(casio.getMarca());
        System.out.println(casio.getModelo());
        System.out.println(casio.menuPredeterminado());
        System.out.println(casio.suma(2, 6));
        System.out.println(casio.resta(9, 5));
        System.out.println(casio.apagar());
        
        System.out.println(sony.encender());
        System.out.println(sony.getMarca());
        System.out.println(sony.getModelo());
        System.out.println(sony.getColor());
        System.out.println(sony.menuModificado());
        System.out.println(sony.cambioCanal());
        System.out.println(sony.volumen());
        System.out.println(sony.apagar());
        
        System.out.println(rd.encender());
        System.out.println(rd.getMarca());
        System.out.println(rd.getModelo());
        System.out.println(rd.getColor());
        System.out.println(rd.menuPredeterminado());
        System.out.println(rd.cambioCanal());
        System.out.println(rd.volumen());
        System.out.println(rd.apagar());
        
        
        
        
        
        
    }
    
}
